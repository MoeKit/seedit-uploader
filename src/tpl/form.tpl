	<input type="hidden" name="class" value="{{classtype}}">
	<input name="file" type="file" id="file" class="mk-file" accept="image/gif,image/jpeg,image/jpg,image/png,image/svg">
	{{img}}
	{{del}}
	{{width}}
	{{height}}
	{{within}}
	{{force}}
</form>